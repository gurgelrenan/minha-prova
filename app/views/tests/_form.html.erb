<%= simple_form_for(@test) do |f| %>
  <%= f.input :name %>
  <%= f.association :teacher, prompt: "Escolha um professor" %>
  <%= f.association :discipline, prompt: "Escolha uma disciplina" %>
  <%= f.input :level, collection: 1..5 %>
  
  <%= f.button :submit, class: "btn btn-primary", value: "Gerar prova" %>
<% end %>
